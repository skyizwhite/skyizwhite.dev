{
  "name": "skyizwhite.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "fix": "yarn -s format && yarn -s lint:fix",
    "format": "prettier --write --loglevel=warn '**/*.{js,jsx,ts,tsx,gql,graphql,json}'",
    "lint": "yarn lint:es && yarn lint:prettier",
    "lint:fix": "yarn -s lint:es:fix; yarn -s lint:prettier:fix",
    "lint:es": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier:fix": "prettier -w '**/*.{js,jsx,ts,tsx}'",
    "postinstall": "typesync && yarn --ignore-scripts",
    "svgo": "svgo -r images"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "daisyui": "^2.15.1",
    "framer-motion": "^6.3.4",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/eslint": "8.4.2",
    "@types/node": "17.0.36",
    "@types/prettier": "^2.6.3",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "4.7.2",
    "typesync": "^0.9.1"
  }
}
